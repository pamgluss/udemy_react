{
    "presets": [
        [
            "@babel/preset-env",
            {
              "modules": false,
              "targets": {
                "browsers": ["> 1%", "last 2 versions"]
              },
              "useBuiltIns": "entry",
              "forceAllTransforms": true
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties"
    ]
}